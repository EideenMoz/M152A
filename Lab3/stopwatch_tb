    %vpi_call 2 71 "$display", "[%t ns] Pausing stopwatch...", $time {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x149f5dc30_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x149f5dc30_0, 0, 1;
    %delay 50000000, 0;
    %vpi_call 2 80 "$display", "[%t ns] Resuming stopwatch...", $time {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x149f5dc30_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x149f5dc30_0, 0, 1;
    %delay 150000000, 0;
    %vpi_call 2 89 "$display", "[%t ns] Enter adjust mode for seconds...", $time {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x149f5df70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x149f5e040_0, 0, 1;
    %delay 150000000, 0;
    %vpi_call 2 96 "$display", "[%t ns] Switching to adjust minutes...", $time {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x149f5e040_0, 0, 1;
    %delay 150000000, 0;
    %vpi_call 2 101 "$display", "[%t ns] Exiting adjust mode...", $time {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x149f5df70_0, 0, 1;
    %delay 200000000, 0;
    %vpi_call 2 107 "$display", "[%t ns] Simulation complete.", $time {0 0 0};
    %vpi_call 2 108 "$stop" {0 0 0};
    %end;
    .thread T_25;
    .scope S_0x149f1e9f0;
T_26 ;
    %vpi_call 2 113 "$monitor", "Time=%t ns | sec_tens=%0d sec_ones=%0d | min_tens=%0d min_ones=%0d", $time, v0x149f5c580_0, v0x149f5c4f0_0, v0x149f5ae30_0, v0x149f5ad80_0 {0 0 0};
    %end;
    .thread T_26;
